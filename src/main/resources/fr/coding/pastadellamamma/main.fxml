<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Point3D?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.coding.pastadellamamma.controller.MainController">
   <children>
      <MenuBar prefWidth="560.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
                <MenuItem fx:id="home" mnemonicParsing="false" text="Accueil" />
                <MenuItem fx:id="menu" mnemonicParsing="false" text="Menu" />
                <MenuItem fx:id="amployeList" mnemonicParsing="false" text="Liste des employés" />
                <MenuItem fx:id="employe" mnemonicParsing="false" text="Ajouter des employés" />
                <MenuItem fx:id="room" mnemonicParsing="false" text="Gestion de salle" />
            </items>
          </Menu>
        </menus>
         <rotationAxis>
            <Point3D />
         </rotationAxis>
         <VBox.margin>
            <Insets bottom="60.0" />
         </VBox.margin>
      </MenuBar>
      <VBox fx:id="content" prefHeight="300.0" prefWidth="500.0" />
   </children>
</VBox>
